{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "installCommand": "npm run install-deps && npm run python-deps",
  "regions": ["fra1"]
}